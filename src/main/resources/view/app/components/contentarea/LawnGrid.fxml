<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="gridPaneContainer" alignment="CENTER" prefHeight="800.0" prefWidth="900.0"
      xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.osmowsis.osmowsisfinalproject.view.controller.LawnGridController"
      style="-fx-background-color: #d3d3c8">
    <children>
        <GridPane fx:id="lawnGridPane" alignment="CENTER" gridLinesVisible="false" HBox.hgrow="ALWAYS"/>
    </children>
    <padding>
        <Insets bottom="14.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</HBox>
